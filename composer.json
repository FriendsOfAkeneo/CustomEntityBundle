{
    "name": "akeneo-labs/custom-entity-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Custom entity bundle",
    "keywords": ["pim", "akeneo", "ecommerce", "integration"],
    "homepage": "http://www.akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Akeneo",
            "homepage": "http://www.akeneo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git",
            "branch": "master"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git",
            "branch": "master"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "akeneo/pim-community-dev": "1.4.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "pdepend/pdepend": "2.1.*",
        "phpmd/phpmd": "1.*",
        "phpspec/phpspec": "2.1.*",
        "fabpot/php-cs-fixer": "^2.0@dev",
        "henrikbjorn/phpspec-code-coverage": "~1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Pim\\Bundle\\CustomEntityBundle\\": ""
        }
    },
    "target-dir": "Pim/Bundle/CustomEntityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
