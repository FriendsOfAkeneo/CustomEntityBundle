{
    "name": "akeneo/custom-entity-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Custom entity bundle",
    "keywords": ["pim", "akeneo", "ecommerce", "integration"],
    "homepage": "http://akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Antoine Guigan",
            "email": "antoine@akeneo.com",
            "homepage": "http://akeneo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git",
            "branch": "master"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "akeneo/pim-community-dev": "dev-maintenance/1.1_backports | 1.2.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\CustomEntityBundle\\": "" }
    },
    "target-dir": "Pim/Bundle/CustomEntityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.x-dev"
        }
    }
}
